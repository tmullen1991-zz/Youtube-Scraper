<div class="row">
    {{#each video}}
    <div class="col s12 m6 l3">
        <div class="card">
            <div class="card-image">
                <img src="https://img.youtube.com/vi/{{this.youtubeID}}/maxresdefault.jpg">
            </div>
            <div class="card-content">
                <h6>{{this.title}}</h6>
            </div>
            <div class="card-action">
                <a href="https://www.youtube.com{{this.url}}" class="blue-text">Link to Video</a>
                <a href="" class="add-playlist">Add to Playlist</a>
            </div>
        </div>
    </div>
    {{/each}}
</div>

<script src="js/app.js"></script>